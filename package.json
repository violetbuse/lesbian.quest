{
  "name": "lesbian.quest",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "prisma:generate": "prisma generate --no-engine",
    "start": "next start",
    "lint": "next lint",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "opennextjs-build": "npm run cf-typegen && opennextjs-cloudflare build",
    "preview": "npm run opennextjs-build && opennextjs-cloudflare preview",
    "deploy": "npm run opennextjs-build && opennextjs-cloudflare deploy",
    "upload": "npm run opennextjs-build && opennextjs-cloudflare upload"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.14.2",
    "@cloudflare/workers-types": "^4.20250410.0",
    "@opennextjs/cloudflare": "^1.0.0-beta.2",
    "@prisma/client": "^6.6.0",
    "@prisma/extension-accelerate": "^1.3.0",
    "@radix-ui/react-slot": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.30.6",
    "drizzle-orm": "^0.41.0",
    "lucide-react": "^0.487.0",
    "next": "15.3.0",
    "prisma": "^6.6.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.2.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.2.5",
    "typescript": "^5",
    "wrangler": "^4.10.0"
  }
}
